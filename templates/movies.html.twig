  {% include('base.html.twig')%}

  <div class="container">
 
 
<table class="table" id="table_starwars"> 
  <thead>
    <tr>
      <th >id</th>      
      <th >name</th>
      <th >episode</th>
      <th >director</th>
      <th >producer</th>
      <th >opening</th>
      <th >release_date</th>
      <th >options</th> 
    </tr>
  </thead>
   <tbody>
{% for m in movies %} 
<form action="" method="GET">
    <tr> 
      <td>{{ m.id }}</td>      
      <td>{{ m.name }}</td>
      <td>{{ m.episode }}</td>
      <td>{{ m.director }}</td>
      <td>{{ m.producer }}</td>
      <td>{{ m.opening }}</td>
      <td>{{ m.releaseDate }}</td>
      <td> <a class="btn btn-warning" href="{{ path('show_movie',{id:m.id})}}"> More</a></td>
    </tr>
</form>
{% endfor %}
  </tbody>
</table>
 </div>
 
